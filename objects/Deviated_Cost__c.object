    <fields>
        <fullName>Deal_Status__c</fullName>
        <externalId>false</externalId>
        <formula>if(Account_Name__c = &quot;&quot;, &quot;Unassigned&quot;, 

if(and(TODAY() &gt; End_Date__c, or(NOT(ISPICKVAL(Rep_Disposition__c, &quot;Extension Granted&quot;)), NOT(ISPICKVAL(Rep_Disposition__c, &quot;No Extension - Deal Ended&quot;)))), &quot;Expired&quot;,

if(and(End_Date__c - TODAY() &lt;= 90, or(NOT(ISPICKVAL(Rep_Disposition__c, &quot;Extension Granted&quot;)), NOT(ISPICKVAL(Rep_Disposition__c, &quot;No Extension - Deal Ended&quot;)))), &quot;Expiring&quot;, 

if(ISPICKVAL(Rep_Disposition__c, &quot;Deviated Cost Review&quot;), &quot;Under Review&quot;, 

if(ISPICKVAL(Rep_Disposition__c, &quot;Extension Granted&quot;), &quot;Won&quot;, 

if(ISPICKVAL(Rep_Disposition__c, &quot;No Extension - Deal Ended&quot;), &quot;Lost&quot;, 

if(ISPICKVAL(Won_Lost__c,&quot;Won&quot;), &quot;Won&quot;, 

if(ISPICKVAL(Won_Lost__c, &quot;Lost&quot;), &quot;Lost&quot;, 

&quot;Status Error&quot;))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Deal Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
