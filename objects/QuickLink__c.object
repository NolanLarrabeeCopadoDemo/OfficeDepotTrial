    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Determines whether the link is active or not. If a link is active, it will be displayed in Quick Links. Otherwise, it won&apos;t display.</description>
        <externalId>false</externalId>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideFromProfiles__c</fullName>
        <description>The profiles to hide this Quick Link from.</description>
        <externalId>false</externalId>
        <label>Hide From Profiles</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>MultiselectPicklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Profiles</valueSetName>
        </valueSet>
        <visibleLines>4</visibleLines>
    </fields>
