    <validationRules>
        <fullName>Hide_Form_Fields_Validation</fullName>
        <active>true</active>
        <description>Communities v2.1 - Only some fields on the form can be hidden, this will block the rest.</description>
        <errorConditionFormula>AND(Hide__c,
NOT(OR(
