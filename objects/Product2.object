        <errorMessage>You cannot specify a List Cost for a recurring product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TZ_Block_list_price_on_recurring_product</fullName>
        <active>true</active>
        <errorConditionFormula>Recurring__c 
&amp;&amp; List_Price__c &gt; 0</errorConditionFormula>
