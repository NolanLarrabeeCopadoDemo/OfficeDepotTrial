    <validationRules>
        <fullName>LBI__Cannot_be_preprocessed_with_rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(LBI__ScoringRule__c))
,
ISPICKVAL(LBI__Status__c, &apos;Preprocessed&apos;)
)</errorConditionFormula>
        <errorDisplayField>LBI__Status__c</errorDisplayField>
        <errorMessage>Status cannot be preprocessed if scoring rule is attached.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LBI__Count_by_One_Only</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LBI__ErrorCount__c &lt;&gt; 0
, LBI__ErrorCount__c &lt;&gt; PRIORVALUE(LBI__ErrorCount__c)+1
, LBI__ErrorCount__c &lt;&gt; PRIORVALUE(LBI__ErrorCount__c))</errorConditionFormula>
        <errorDisplayField>LBI__ErrorCount__c</errorDisplayField>
        <errorMessage>Error Count can only increment by 1.</errorMessage>
    </validationRules>
