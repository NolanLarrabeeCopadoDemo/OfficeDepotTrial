    <validationRules>
        <fullName>SoftHeaderVal_Length_Verification</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(ISPICKVAL(SoftHeader_Type__c,&apos;Desktop&apos;),((LEN(SoftHeader_Value__c)) &gt; 12)),
AND(ISPICKVAL(SoftHeader_Type__c,&apos;Cost Center&apos;),((LEN(SoftHeader_Value__c)) &gt; 20)),
AND(ISPICKVAL(SoftHeader_Type__c,&apos;Release&apos;),((LEN(SoftHeader_Value__c)) &gt; 20))
)</errorConditionFormula>
        <errorMessage>Desktop Value should not be more than 12 characters.
Cost Center value shouldn&apos;t be  more than 20 characters.
Release value shouldn&apos;t be  more than 20 characters.</errorMessage>
    </validationRules>
