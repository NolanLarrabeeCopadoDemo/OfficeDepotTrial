    <validationRules>
        <fullName>TAV4__Manager_Assignments</fullName>
        <active>true</active>
        <description>A sales manager role cannot be assigned to an account, lead or geo unit.</description>
        <errorConditionFormula>TAV4__Sales_Area__r.TAV4__Is_Assignable__c = TAV4__Sales_Rep__r.TAV4__Is_Manager__c</errorConditionFormula>
        <errorMessage>A sales manager may not be assigned to a sales rep position, and a sales rep may not be assigned to a non-assignable position in the sales hierarchy.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TAV4__Sales_rep_in_alignment</fullName>
        <active>true</active>
        <errorConditionFormula>TAV4__Sales_Area__r.TAV4__Alignment__c  !=  TAV4__Sales_Rep__r.TAV4__Alignment__c</errorConditionFormula>
        <errorDisplayField>TAV4__Sales_Rep__c</errorDisplayField>
        <errorMessage>The sales rep must be assigned to the alignment that the sales area is part of.</errorMessage>
    </validationRules>
