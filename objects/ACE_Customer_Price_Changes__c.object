    <fields>
        <fullName>Cust_Notif_Approved_Month__c</fullName>
        <externalId>false</externalId>
        <formula>IF((MONTH(Customer_Notification_Date_Approved__c)) = 1.0, 1.0,
IF((MONTH(Customer_Notification_Date_Approved__c)) = 2.0, 1.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 3.0, 1.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 4.0, 2.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 5.0, 2.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 6.0, 2.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 7.0, 3.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 8.0, 3.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 9.0, 3.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 10.0, 4.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 11.0, 4.0, 
IF((MONTH(Customer_Notification_Date_Approved__c)) = 12.0, 4.0, 
null))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Cust_Notif_Approved_Month</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
