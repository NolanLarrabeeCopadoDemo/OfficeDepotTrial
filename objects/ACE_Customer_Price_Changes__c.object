        <externalId>false</externalId>
        <formula>IF(AND(Implementation_Status_Code__c == &apos;SCH&apos;,SFDC_Notification_Flag__c == &apos;3&apos;),&apos;Scheduled - Pending Customer Approval&apos;,
 IF(Implementation_Status_Code__c == &apos;SCH&apos;,&apos;Scheduled&apos;,   
 IF( Implementation_Status_Code__c == &apos;SCH-PEN&apos;,&apos;Schedule May Change, Approval Pending&apos;, 
 IF( Implementation_Status_Code__c == &apos;DATA-SENT&apos;,&apos;Processing&apos;, 
 IF( Implementation_Status_Code__c == &apos;DATA-SUCCESS&apos;,&apos;Executed&apos;, 
 IF( Implementation_Status_Code__c == &apos;DATA-FAILED&apos;,&apos;Failed&apos;, 
 IF( Implementation_Status_Code__c == &apos;NOTIFY&apos;,
      IF( TODAY() &lt;  Sales_Price_Change_Effective_Date_Approv__c , &apos;AT RISK - Awaiting Rep Input of Customer Approval&apos;,  
             IF( TODAY() &gt;=  Sales_Price_Change_Effective_Date_Approv__c , &apos;LATE - Awaiting Rep Input of Customer Approval&apos;,
             &apos;&apos;)
         ),  
 
      IF(Implementation_Status_Code__c == &apos;COMPL&apos;,  
         IF(TODAY() &lt;  Sales_Price_Change_Effective_Date_Approv__c, &apos;Scheduled&apos;,    
         IF(TODAY() &gt;=  Sales_Price_Change_Effective_Date_Approv__c, &apos;Scheduled&apos;,
             &apos;&apos; )), 
      &apos;&apos;)
   ) 
 ))))))</formula>
