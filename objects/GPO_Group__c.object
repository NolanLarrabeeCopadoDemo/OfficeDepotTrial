    <validationRules>
        <fullName>CommuNameValid</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX( Community_Name__c  , &quot;^[a-zA-Z0-9-/ ]*$&quot;))</errorConditionFormula>
        <errorDisplayField>Community_Name__c</errorDisplayField>
        <errorMessage>Invalid Community Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deactivation_not_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>if (NOT(ISNEW()) &amp;&amp; ISCHANGED( Activated__c ) &amp;&amp; Activated__c == false, true,false)</errorConditionFormula>
        <errorDisplayField>Activated__c</errorDisplayField>
        <errorMessage>An Active Community Cannot be Deactivated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Default_Group_Name_cannot_be_changed</fullName>
        <active>false</active>
        <description>Communities v2: All SOQL calls for the Default Group reference it by Community_Name__c = &apos;Default Group&apos;, so this cannot be changed for any reason.</description>
        <errorConditionFormula>AND(ISCHANGED( Community_Name__c ),(Name = &apos;00008&apos;) )</errorConditionFormula>
        <errorMessage>Default Group&apos;s Name cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Generic_GPO_must_have_Generic_Message</fullName>
        <active>false</active>
        <description>Communities v2: Rule that enforces the selected_message__c field on the Default Group to always be set to the Generic Message.</description>
        <errorConditionFormula>AND(Community_Name__c = &apos;Default Group&apos;,Selected_Message__r.Name != &apos;Generic Group Message&apos;)</errorConditionFormula>
        <errorMessage>Default Group Message cannot be changed.</errorMessage>
    </validationRules>
