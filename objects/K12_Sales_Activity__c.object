        <fullName>Variance__c</fullName>
        <externalId>false</externalId>
        <formula>if (

( text(Activity_Type__c) == &apos;WatsonOpportunity&apos; || text(Activity_Type__c) ==&apos;Appointment&apos; ) ,Adj_Actual__c-Quota__c,

if ( (text(Activity_Type__c) == &apos;Amount&apos; ) ,( Opp_Value_Total__c-Quota__c),null
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
