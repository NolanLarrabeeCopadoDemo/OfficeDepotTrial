<apex:component controller="ACE_Customer_List" allowDML="true">      
    <Style>
        //.headStyle {border-left: 1px solid #FFFFFF; background-color:#333333; color: Black;}
        .col1 {width:90%;}
        .col2 {width:10%;text-align: right; align:right;}

        .btn1{width:50%;text-align: left; align:left;}
        .btn2 {width:50%;text-align: right; align:right;}
        .btn3{width:50%;text-align: left; align:left;}
        .btn4 {width:50%;text-align: right; align:right;}
        .expandCollapse{
        background-color:#0099CC;
        width:auto;
        height:auto;
        }
    </Style>    
    
<apex:form id="form">      
    <apex:pageBlock id="block">                    
          <apex:pageMessages ></apex:pageMessages>             
            
            <apex:outputPanel id="search1">  
                                <apex:outputPanel >
                                    <B>Search by Customer Number or Name: </B> <apex:inputText id="text1" value="{!searchText}" tabindex="0" onkeypress="if(event.keyCode==13){Refresh_3(this); return false;}"/> &nbsp;&nbsp;&nbsp;
                                    <apex:commandButton action="{!SearchRecords}" value="Search" reRender="table" status="search_Status3" tabindex="1"/>
                               
                                    <div id="contentLoading3" style="display:none;position:fixed;top:50%;right:50%;" >
                                        <div style="text-align: center;">
                                          <img src="/resource/1386251299000/Ajax_Loading" alt="Loading graphic"  height="100" width="100"/>
                                        </div>
                                    </div> 
                
                                    <apex:actionStatus id="search_Status3" onstart="loading3(true)" onstop="loading3(false)" startStyle="color:red;"/>  
                                    
                                </apex:outputPanel>   
                                
                                <apex:actionFunction name="Refresh_rec3" action="{!SearchRecords}" reRender="table" status="search_Status3"/>
                                 
                                <apex:outputPanel style="text-align: right;float:right;" rendered="{!IF(AccessLevel=='GENERAL',true,false)}">
                                    <apex:selectList value="{!OwnerValue}" multiselect="false" size="1" onchange="Refresh_3();">
                                        <!--  <apex:actionSupport event="onchange" action="{!SearchRecords}">
                                        </apex:actionSupport> -->
                                        <apex:selectOption itemValue="Owned by me" itemLabel="Owned by me"/>
                                        <apex:selectOption itemValue="Owned by my team" itemLabel="Owned by my team"/>
                                        <apex:selectOption itemValue="Owned by others" itemLabel="Owned by others"/>
                                        <apex:selectOption itemValue="All" itemLabel="All"/>
                                    </apex:selectList> 
                                </apex:outputPanel>   
                                <apex:outputPanel style="text-align: right;float:right;" rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}">
                                    <apex:selectList value="{!OwnerValue}" multiselect="false" size="1" >
                                        <apex:actionSupport event="onchange" action="{!SearchRecords}">
                                        </apex:actionSupport>
                                        <apex:selectOption itemValue="All" itemLabel="All"/>
                                        <apex:selectOption itemValue="PendingApproval" itemLabel="Pending Approval"/>
                                        <apex:selectOption itemValue="Unassigned" itemLabel="Unassigned"/>
                                        <apex:selectOption itemValue="Active" itemLabel="Active"/>
                                    </apex:selectList> 
                                </apex:outputPanel>   
            </apex:outputPanel>                  
            
            <apex:outputPanel id="table">                                
                <apex:pageBlockTable value="{!clsCusListTable}" var="customer" width="100%" id="pbTab">           
                    
                    <apex:column rendered="false"> 
                        <apex:facet name="header">Send <br/>Notification</apex:facet>                       
                        <apex:inputCheckbox value="{!customer.NOTCHECK}"/>
                    </apex:column>
                    
                      
                    <apex:column rendered="{!IF(AccessLevel!='GENERAL',true,false)}" headervalue="Actions">   
                         
                         <apex:panelGrid columns="3"> 
                         <!--   <apex:actionRegion >       
                               <apex:commandbutton value="Details" action="{!findskus}" rerender="skuPanel" onclick="hideDiv();" oncomplete="renderDiv2();">
                                  <apex:param name="selectedCustomer" value="{!customer.id}"/>
                                  <apex:param name="selectedCustomerName" value="{!customer.Customer}"/>
                               </apex:commandbutton>
                           </apex:actionRegion>                                                                                 
                                  -->                                                
                           <apex:outputPanel rendered="{!IF(customer.showUD == 'Gen',true,false)}">
                             <apex:actionRegion >
                             <apex:commandLink value="Approve" action="{!ApproveRec}" onclick="return confirm('Please confirm! The selected customer record will be Approved for Remediation.');">
                                    <apex:param name="RecSel" value="{!customer.id}" assignTo="{!RecSel}"/>
                             
                             </apex:commandLink>    
                             </apex:actionRegion>
                           </apex:outputPanel>
                           
                           <apex:outputPanel rendered="{!IF(customer.showUD == 'Gen',true,false)}">
                             <apex:actionRegion >
                             <apex:commandLink value="Reject" action="{!RejectRec}" onclick="return confirm('Please confirm! The selected customer record will be Rejected for Remediation.');">
                                    <apex:param name="RecSel" value="{!customer.id}" assignTo="{!RecSel}"/>
                               
                             </apex:commandLink>    
                             </apex:actionRegion>
                           </apex:outputPanel>                                                       
                           
                           <apex:outputPanel >
                             <apex:actionRegion >
                             <apex:commandlink value="Download" onclick="window.open('/apex/apex/ACE_Export_SKU_Records?id={!customer.id}');"/>
                             </apex:actionRegion>
                           </apex:outputPanel>
                           
                           <apex:outputPanel >
                                 <apex:actionregion >
                                 <apex:commandLink onclick="window.open('/apex/ACEUpload_Page?selectedCustomer={!customer.id}&selectedCustomerName={!customer.Customer}');return false;" value="Upload"  rendered="{!customer.hostedCatalogHold || customer.showUD == 'Approved'}">
                                     <apex:param name="selectedCustomer" value="{!customer.id}"/>                                     
                                 </apex:commandLink>
                                 </apex:actionregion>
                            </apex:outputPanel>                                                     
                           
                         </apex:panelGrid>
                    </apex:column>
                    
                    <apex:column rendered="{!IF(AccessLevel=='GENERAL',true,false)}">
                        <apex:facet name="header">Release <br/>Early</apex:facet>                    
                        <apex:inputCheckbox id="chRelease" value="{!customer.RELCHECK}" disabled="{!!customer.ISRELEASEALLOWED}"/>
                    </apex:column>
                    
                    <apex:column value="{!customer.customerNumber}"> <!-- 00826597  -->   
                      <apex:facet name="header">Customer #
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Customer #</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="customerid" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAscCustID" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'customerid'}"/> 
                          <apex:image id="sortInDescCustID" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'customerid'}"/>
                      </apex:commandLink>
                      </apex:facet>
                    </apex:column>
                    
                    <apex:column >
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Customer</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="Customer" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'Customer'}"/> 
                          <apex:image id="sortInDesc" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'Customer'}"/>
                      </apex:commandLink>                      
                      </apex:facet>
                      <a href="/{!customer.ID}" id="{!customer.ID}" target="_top">
                      {!customer.CUSTOMER}
                      </a>                                          
                    </apex:column>
                    
                    <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" headerValue="Verified">
                      <center>
                       <apex:image value="{!$Resource.ACECheckBox}" rendered="{!NOT(customer.remediationHold) && customer.showUD != 'Gen'}" style="height:20px;width:20px;"/>
                      </center>
                       <center>
                       <apex:image value="{!$Resource.AceUncheck}" rendered="{!customer.remediationHold && customer.showUD != 'Gen'}" style="height:20px;width:20px;"/>
                      </center>
                    </apex:column>
                    
                    <apex:column rendered="{!IF(AccessLevel=='HOSTED',true,false)}" headerValue="Verified">
                     <center>
                       <apex:image value="{!$Resource.ACECheckBox}" rendered="{!NOT(customer.hostedCatalogHold)}" style="height:20px;width:20px;"/>
                     </center>
                     <center>
                       <apex:image value="{!$Resource.AceUncheck}" rendered="{!customer.hostedCatalogHold}" style="height:20px;width:20px;"/>
                     </center>
                    </apex:column>
                    
                    
                    
                    <apex:column value="{!customer.OWNERNAME}" rendered="{!RenderRep}">
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Rep</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="Reps" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAscRep" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'Reps'}"/> 
                          <apex:image id="sortInDescRep" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'Reps'}"/>
                      </apex:commandLink>                      
                      </apex:facet>
                    </apex:column>
                    
                    <apex:column value="{!customer.CORES}">
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText ># Impacted<br/>Cores</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="Cores" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc1" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'Cores'}"/> 
                          <apex:image id="sortInDesc1" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'Cores'}"/>
                      </apex:commandLink>                      
                      </apex:facet>
                    </apex:column>
                    
                    <apex:column value="{!customer.SKUS}"> <!-- 00826597  -->   
                      <apex:facet name="header"># Impacted<br/>SKUs
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText ># Impacted<br/>SKUs</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="SKU" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc2" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'SKU'}"/> 
                          <apex:image id="sortInDesc2" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'SKU'}"/>
                      </apex:commandLink>
                      </apex:facet>
                    </apex:column>
                    
                    <apex:column >
                        <apex:facet name="header">
                        <apex:commandLink action="{!sortCust}" reRender="table">
                            <apex:outputText >Number of SKUs<br/>Governed By Active<br/>Written Agreement</apex:outputText>  &nbsp; 
                            <apex:param name="fieldName" value="NoOfSKUGovernedByActiveWrittenAgreement" assignTo="{!getFieldName}"/>
                            <apex:image id="sortInAscWritnAgr" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'NoOfSKUGovernedByActiveWrittenAgreement'}"/> 
                            <apex:image id="sortInDescWritnAgr" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'NoOfSKUGovernedByActiveWrittenAgreement'}"/>
                        </apex:commandLink> 
                        </apex:facet>
                        <apex:outputText value="{!customer.NoOfSKUGovernedByActiveWrittenAgreement}">
                        </apex:outputText>
                    </apex:column>
                    
                    <apex:column headerValue="Avg Change">
                        <apex:facet name="header">Avg Change</apex:facet>
                        <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.AVGCHANGE}"/>
                        </apex:outputText>
                    </apex:column> 
                    
                    
                    
                    <apex:column > <!-- 00826597  -->                       
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Annual Spend</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="annualspend" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc3" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'annualspend'}"/> 
                          <apex:image id="sortInDesc3" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'annualspend'}"/>
                      </apex:commandLink> 
                      </apex:facet>
                      <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.ANNUALSPEND}"/>
                      </apex:outputText>                      
                    </apex:column>
                    
                   <apex:column > <!-- value="{! IF((customer.ANNSPEND_CHNGEAMNT == null), '', '$') }{!customer.ANNSPEND_CHNGEAMNT}">                       -->
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Annual Spend <br/>Change</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="annualspndchange" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc4" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'annualspndchange'}"/> 
                          <apex:image id="sortInDesc4" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'annualspndchange'}"/>
                      </apex:commandLink> 
                      </apex:facet>
                      <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.ANNSPEND_CHNGEAMNT}"/>
                      </apex:outputText>
                    </apex:column>
                        
                                    
                    <apex:column headerValue="Spend Impact"  >
                        <apex:facet name="header">Spend Impact</apex:facet>
                        <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.SPNDIMPACT}"/>
                      </apex:outputText>
                    </apex:column>
                    
                    <apex:column > <!-- value="{! IF((customer.SRMIMPACT == null), '', '$') }{!customer.SRMIMPACT}">                        -->
                      <apex:facet name="header">
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >SRM Impact</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="srmimpact" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAsc5" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'srmimpact'}"/> 
                          <apex:image id="sortInDesc5" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'srmimpact'}"/>
                      </apex:commandLink> 
                      </apex:facet>
                      <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.SRMIMPACT}"/>
                      </apex:outputText>
                    </apex:column> 
                    
                    
                     <apex:column headerValue="IGM Gross ($)" rendered="{!RenderIGM}">
                         <apex:outputText value="${0, number, ###,###,###,##0.00}">
                            <apex:param value="{!customer.NEWIGMDOLLAR}"/>
                       </apex:outputText>
                     </apex:column> 
                     <apex:column headerValue="IGM Gross (%)" rendered="{!RenderIGM}">
                         <apex:outputText value="{0, number, ###,###,###,##0.00}%">
                            <apex:param value="{!customer.NEWIGMPERCENT}"/>
                       </apex:outputText>
                     </apex:column>
                    
                                   
                      <apex:column ><!-- 00826597  -->   
                        
                      <apex:facet name="header">Notification <br/>Status
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Notification <br/>Status</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="notifStatus" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAscNotifStatus" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'notifStatus'}"/> 
                          <apex:image id="sortInDescNotifStatus" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'notifStatus'}"/>
                      </apex:commandLink>
                      </apex:facet>
                          <center>
                        <apex:outputField value="{!customer.ts.Notification_Status_Image__c}" rendered="{!customer.ts.Notification_Status_Image__c != '<img src=\" \" alt=\" \" height=\"25\" width=\"25\" border=\"0\"/>'}" /> 
                        
                        <br></br>
                        <apex:outputField value="{!customer.ts.Notification_Status3__c}" style="color:grey"/>                        
                        </center>
                    </apex:column>
                      
                    <apex:column >
                        <apex:facet name="header">
                        <apex:commandLink action="{!sortCust}" reRender="table">
                            <apex:outputText >Notification<br/> Date</apex:outputText>  &nbsp; 
                            <apex:param name="fieldName" value="notifydate" assignTo="{!getFieldName}"/>
                            <apex:image id="sortInAsc7" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'notifydate'}"/> 
                            <apex:image id="sortInDesc7" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'notifydate'}"/>
                        </apex:commandLink> 
                        </apex:facet>
                        <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                            <apex:param value="{!customer.NOTIFYDATE}" /> 
                        </apex:outputText>
                    </apex:column>  
                    
                    <apex:column ><!-- 00826597  -->   
                        
                      <apex:facet name="header">Price Change Status
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Price Change Status</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="pricechangestatus" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAscPrcChgSt" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'pricechangestatus'}"/> 
                          <apex:image id="sortInDescPrcChgSt" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'pricechangestatus'}"/>
                      </apex:commandLink>
                      </apex:facet>
                          <center>
                        <apex:outputField value="{!customer.ts.Implementation_Status_Image__c}"/>
                        <br></br>
                        <apex:outputField value="{!customer.ts.Implementation_Status3__c}" style="color:grey"/>                        
                        </center>  
                    </apex:column>
                        
                    <apex:column >
                        <apex:facet name="header">
                        <apex:commandLink action="{!sortCust}" reRender="table">
                            <apex:outputText >Price Change<br/>Effective Date</apex:outputText>  &nbsp; 
                            <apex:param name="fieldName" value="effectiveDate" assignTo="{!getFieldName}"/>
                            <apex:image id="sortInAsc6" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'effectiveDate'}"/> 
                            <apex:image id="sortInDesc6" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'effectiveDate'}"/>
                        </apex:commandLink> 
                        </apex:facet>
                        <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                            <apex:param value="{!customer.APPROVDDATE}" /> 
                        </apex:outputText>
                    </apex:column>
                    
                <!-- 00826595  -->    
                <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" headerValue="Request Type">
                      <apex:outputText value="{!customer.requestType}" />
                 </apex:column>
                
                <!-- 00826595  -->
                <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" headerValue="WRF #" >
                      <a href="/{!customer.wrfID}" id="{!customer.wrfID}" target="_blank">
                      {!customer.wrfID}
                      </a>                                          
                </apex:column>
                
                <!-- 00826595  -->
                 <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" ><!-- 00826597  -->   
                        <apex:outputText value="{!customer.complianceWrfTaskClosed}">
                        </apex:outputText>
                      <apex:facet name="header">
                      <apex:outputText >Compliance <br/> Task Status</apex:outputText>
                      </apex:facet>
                    </apex:column>
                    
                <!-- 00826595  -->
                <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" headerValue="Segment" >
                      <apex:outputText value="{!customer.segment}" />                                         
                </apex:column>
                    
                <!-- 00826595  -->
                 <apex:column rendered="{!IF(AccessLevel=='REMEDIATION',true,false)}" ><!-- 00826597  -->   
                        <apex:outputText value="{!customer.remedationTaskAssignee}">
                        </apex:outputText>
                      <apex:facet name="header">
                      <apex:outputText >Remed. <br/> Task Assignee</apex:outputText>
                      </apex:facet>
                    </apex:column>
        
        		<apex:column ><!-- 00826597  -->   
                        <apex:outputText value="{!customer.sfdcBucket}">
                        </apex:outputText>
                      <apex:facet name="header">Request
                      <apex:commandLink action="{!sortCust}" reRender="table">
                          <apex:outputText >Req. Bucket</apex:outputText>  &nbsp; 
                          <apex:param name="fieldName" value="sfdcBucket" assignTo="{!getFieldName}"/>
                          <apex:image id="sortInAscSFDCBucket" value="{!$Resource.ACE_Up_Sort}" rendered="{!tabAsc && getFieldName == 'sfdcBucket'}"/> 
                          <apex:image id="sortInDescSFDCBucket" value="{!$Resource.ACE_Down_Sort}" rendered="{!!tabAsc && getFieldName == 'sfdcBucket'}"/>
                      </apex:commandLink>
                      </apex:facet>
                    </apex:column>
                    
                                                             
                                                          
                </apex:pageBlockTable>
                
                <apex:panelGrid id="buttons2" width="100%" columns="2" columnClasses="btn3,btn4">
                    <apex:outputPanel >
                        <apex:commandButton action="{!prevPage}" value="<< Previous <<" status="search_Status3" rendered="{!DisablePrevious}" reRender="table,out1" style="color:white;background-color:#0099CC;background-image:none;-moz-border-radius:10px 10px 10px 10px;-webkit-border-radius:10px 10px 10px 10px; border-radius:10px 10px 10px 10px;"/>
                    </apex:outputPanel>
                    <apex:outputPanel >
                        <apex:commandButton action="{!nextPage}" value=">> Next >>" status="search_Status3" rendered="{!DisableNext}" reRender="table,out1" style="color:white;background-color:#0099CC;background-image:none;-moz-border-radius:10px 10px 10px 10px;-webkit-border-radius:10px 10px 10px 10px; border-radius:10px 10px 10px 10px;"/>
                    </apex:outputPanel>
                </apex:panelGrid>
                
                <apex:panelGrid width="100%" columns="2" columnClasses="btn3,btn4" rendered="{!IF(AccessLevel=='GENERAL',true,false)}">                 
                    <apex:outputPanel >
                        <apex:commandButton onclick="showOverlay3(); return false;" value="Submit" style="font-weight:bold;color:white;background-color:#006600;background-image:none;"/>                    
                        <apex:actionStatus startText="Submitting...." stopText="" id="doitStatus"/>                         
                    </apex:outputPanel>                       
                    <apex:outputPanel ></apex:outputPanel>
                    <apex:outputPanel id="errPanel" style="color:red;font-weight:italic">
                        {!IF(renderErr = true,'You must select at least one (1) record to release early.','')}
                    </apex:outputPanel> 
                    <apex:outputpanel ></apex:outputpanel>
                </apex:panelGrid> 
                
            </apex:outputPanel>
            
            <!-- <apex:outputPanel >            
                        <left><apex:outputText value="Note: For customers who have pending exceptions or where notification has already been sent, the Release Early functionality will be disabled." style="color:Black; font-weight:bold;"/></left>
                    </apex:outputPanel> -->
          
          <!-- Block for Upload Functionality Starts -->              
