             <apex:pageBlock rendered="{!IsRemedy}" title="Remediation - File Upload">
                 <center>                 
                 <apex:inputFile value="{!contentFile}" filename="{!fileNameRem}"  />                  
                 <apex:commandButton action="{!Remedy_ReadFile}" value="Upload File" id="theButton" style="width:70px;"/>                
                 </center>
             </apex:pageBlock> 
