            var elem = document.getElementById(val.id);
             //alert('elem:' + elem);
            elem.focus();
            
            
        }
        function loading3(val) {
            if (val) {
              document.getElementById('contentLoading3').style.display = 'block';
              
            }
            else {
              document.getElementById('contentLoading3').style.display = 'none';
             
            }
          }
    </script>  
