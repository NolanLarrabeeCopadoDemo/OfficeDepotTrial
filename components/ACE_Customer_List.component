                 <center>
                 <apex:inputFile id="host1" value="{!content1}" filename="{!fileNameHost}" /> 
                 <apex:commandButton action="{!Hosted_ReadFile}" value="Upload File" id="theButton1" style="width:70px;" rerender="form"/>
                 </center>
             </apex:pageBlock>
         </div> -->
      <!-- Upload Functionality Block Ends Here -->
      
          <div id="div2"> <!-- for Hide and show on clicking the upload button in the table -->
          <apex:outputPanel id="skuPanel">                  
