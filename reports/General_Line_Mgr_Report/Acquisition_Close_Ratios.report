<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>IF(Opportunity.Win__c:SUM=0,0,
Opportunity.Win_amt__c:SUM/Opportunity.Win__c:SUM)</calculatedFormula>
        <datatype>currency</datatype>
        <developerName>FORMULA6</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Avg Deal Size</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(Opportunity.Appointments_cnt__c:SUM = 0, 0,
Opportunity.Proposal_cnt__c:SUM/Opportunity.Appointments_cnt__c:SUM )</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA7</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Opp to Prop Ratio</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(Opportunity.Proposal_cnt__c:SUM=0, 0,
WON:SUM /Opportunity.Proposal_cnt__c:SUM)</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA8</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Prop to Win Ratio</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF(Opportunity.Win__c:SUM=0,0,
Opportunity.No_Ordering__c:SUM / Opportunity.Win__c:SUM)</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA9</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Win to Ordering Ratio</masterLabel>
        <scale>0</scale>
    </aggregates>
