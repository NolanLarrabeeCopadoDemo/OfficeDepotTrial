                    f1.first_name__c = document.getElementById('regFirstName').value;
                    f1.last_name__c = document.getElementById('regLastName').value;
                    f1.email__c = document.getElementById('regEmail').value;
                    f1.Phone__c = document.getElementById('regPhone').value;
                    f1.address1__c = document.getElementById('regAddress1').value;
                    f1.address2__c = document.getElementById('regAddress2').value;
                    f1.city__c = document.getElementById('regCity').value;
                    f1.state__c = document.getElementById('regState').value;
                    f1.zip__c = document.getElementById('regBillingZip').value;
                    f1.URL__c = document.getElementById('regWebsite1').value;
                    f1.WCW_Range__c = document.getElementById('regNumEmployees').value;
                    f1.Business_Name__c = document.getElementById('regCompanyName').value;
                  //  f1.DUNS__c = document.getElementById('regDuns').value;
                    f1.Notes__c = document.getElementById('regNotes').value;
                    f1.Status__c = 'Pending';
                    var pwd = document.getElementById('regPassword').value;

                    if (document.getElementById('g-recaptcha-response').value != null) {
                        var s = document.getElementById('g-recaptcha-response').value;
                        f1.Recaptcha__c = s.substring(0, 90);
                    }

                    Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.com2GPOCommunityLandingController.insertLeads}', f1, id1, pwd, 
                        function(result, event) {
                            console.log(result);
                            var resultString = '';

                            if (event.status) {
                                resultString = result;
                                document.getElementById("regerrormessage").innerHTML = "Successful Registration";
                                //window.location.replace("/com2ThankYouPage?id=" + id1);
                                window.location.replace("/com2ThankYouPage?id=" + id1 + "&src=reg");
                            } else if (event.type === 'exception') {
                                resultString = event.message + "  " + event.where;
                                document.getElementById("regerrormessage").innerHTML = resultString;
                            } else {
                                resultString = event.message;
                                document.getElementById("regerrormessage").innerHTML = resultString;
                            }
                        });
                }
                return false;
            }
    
   

