<apex:define name="NewCustomerRegistration">
    <!-- BEGIN New Custom Registration -->
    <div class="modal fade" id="ODFLead">
        <div class="modal-dialog">
            <apex:outputPanel rendered="{!showForm}">
                <!-- BEGIN NEW REGISTRATION FORM STEP 1 thru 3 -->
                    <div id="registrationPage" class="mainWrapper odgrid_12 gw12 alpha omega">
                        <!-- Main section -->
                        <form id="registrationForm" name="registrationForm" class="gw8 register_form" method="post">
                                    <div id="regStep1" class="reg_step">
                                        <section class="p24">
                                            <h1> New User Registration </h1>
                                            <p class="mb24" id="greyText"> Enter your information to create a new user profile. </p>
                            
                                            
                                            <apex:outputLabel value="{! UserRegistrationFieldsMap['Login Name'] }" />
                                            <input type="text" minlength="6" maxlength="80" name="regLoginName" id="regLoginName" required="required" onkeypress="return IsLoginName(event);" onfocus="type1CoreM('{!pageid}','Step1of3','CommunitiesRegistration')"/>
