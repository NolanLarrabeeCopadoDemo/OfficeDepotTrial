                     if (document.getElementById('regPassword').value=='') {
                       document.getElementById('regPassword').style.border = '1px solid #f00';
                       document.getElementById('regPassword').style.backgroundColor = '#f2bfbf';
                       document.getElementById('regPasswordEM').style.display = 'inline';           
                        
                       ret=false;
                    }
                   
                     if (document.getElementById('regConfirmPassword').value=='') {
                       document.getElementById('regConfirmPassword').style.border = '1px solid #f00';
                       document.getElementById('regConfirmPassword').style.backgroundColor = '#f2bfbf';
