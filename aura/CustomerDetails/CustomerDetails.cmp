<aura:component controller="AccountSetUpController"
    implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
    access="global">
    <ltng:require styles="{!$Resource.popupCSS}" />
    <aura:attribute name="taxfiles" type="List"/>
    <aura:attribute name="fileName" type="String" />
    <aura:attribute name="base64Data" type="String" />
    <aura:attribute name="contentType" type="String" />
    <aura:attribute name="showFileName" type="Boolean" default="false"/>
    <aura:attribute name="showBillTo" type="Boolean" default="true" />
    <aura:attribute name="showShipTo" type="Boolean" />
    <aura:attribute name="showPrimaryContact" type="Boolean" />
    <aura:attribute name="showBillingPref" type="Boolean" />
    <aura:attribute name="showTax" type="Boolean" />
    <aura:attribute name="showSubmitRequest" type="Boolean" />
    <aura:attribute name="toggleePDF" type="boolean" />
    <aura:attribute name="togglePaper" type="boolean" />
    <aura:attribute name="toggle1change" type="boolean" />
    <aura:attribute name="toggletax" type="boolean" />
    <aura:attribute name="togglegov" type="boolean" default="false" />
    <aura:attribute name="togglePO" type="boolean" default="false" />
    <aura:attribute name="togglePOD" type="boolean" default="false" />
    <aura:attribute name="togglePOCost" type="boolean" default="false" />
    <aura:attribute name="togglePODeskTop" type="boolean" default="false" />
    <aura:attribute name="togglePORelease" type="boolean" default="false" />
    <aura:attribute name="toggleDelivery" type="boolean" default="false" />
    <aura:attribute name="checkboxValueMap" type="List" />
    <aura:attribute name="errorMessages" type="String" />
    <aura:attribute name="errorMessagedml" type="String" />
    <aura:attribute name="isRecordCreated" type="Boolean" default="false" />
    <aura:attribute name="validationError" type="Boolean" default="false" />
    <aura:attribute name="validationErrordml" type="Boolean" default="false" />
    <aura:attribute name="DML_Error" type="String" default="" />
    <aura:attribute name="States" type="List"
        default="[ 'AL','AK','AZ','AR','CA','CO','CT','DC','DE','FL','GA','HI','ID','IL','IN','IA','KS','KY','LA','ME','MD','MA','MI','MN','MS','MO','MT','NE','NV','NH','NJ','NM','NY','NC','ND','OH','OK','OR','PA','RI','SC','SD','TN','TX','UT','VT','VA','WA','WV','WI','WY']" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="subrecordIdLocator" type="Id" access="global" />
    <!-- Account information to serach Org if nay and create a prospect Account -->
    <aura:attribute name="accRec" type="Account_Setup_Request__c"
        default="{'sobjectType': 'Account_Setup_Request__c'}" />
    <aura:attribute name="taxAttachment" type="Attachment"
        default="{'sobjectType': 'Attachment'}" />
    <aura:registerEvent name="processIndicatorEvent" type="c:ProcessIndicatorEvent" />
    <!-- In any required field information missed while seraching account Infor -->
    <aura:attribute name="requiredError" type="boolean" />
    <aura:attribute name="Spinner" type="boolean" default="false" />
    <aura:handler event="aura:waiting" action="{!c.showSpinner}" />
    <aura:handler event="aura:doneWaiting" action="{!c.hideSpinner}" />
    <aura:attribute name="inputError" type="boolean" />
    <aura:attribute name="errorMessage" type="String" />
    <aura:attribute name="ErrorFinal" type="String" />
    <aura:attribute name="DMLValue" type="String" />
    <aura:attribute name="selectedDML" type="String" />
    <aura:attribute name="BillToError" type="String" />
    <aura:attribute name="BillToError1" type="String" />
    <aura:attribute name="BillToErrorAP" type="String" />
    <aura:attribute name="BillToErrorE1" type="String" />
    <aura:attribute name="BillToErrorE2" type="String" />
    <aura:attribute name="BillToErrorFax" type="String" />
    <aura:attribute name="BillToErrorFax1" type="String" />
    <aura:attribute name="TINError" type="String" />
    <aura:attribute name="ODEmailError" type="boolean" />
    <aura:attribute name="DML" type="List" />
    <aura:attribute name="AltDML" type="List" />
    <aura:attribute name="AltDMLs" type="String" />
    <aura:attribute name="AltDMLkey" type="String" />
    <aura:attribute name="AltDMLvalue" type="String" />
    <aura:attribute name="DMLs" type="boolean" default="true" />
    <aura:attribute name="ALT" type="String" />
    <aura:attribute name="dmlRec" type="DML__c" default="{'sobjectType': 'DML__c'}" />
    <aura:attribute name="AltdmlRec" type="Alternate_DML__c" default="{'sobjectType': 'Alternate_DML__c'}" />
    <aura:attribute name="disableDMLCreate" type="Boolean" default="true" />
    <aura:attribute name="documentLink" type="String" default="" />

    <aura:registerEvent name="logError" type="c:ErrorNotificationEvent" />
    <aura:handler name="logError" event="c:ErrorNotificationEvent" action="{!c.reportError}" />

    <div style="margin: auto;width: 75%;">
        <aura:if isTrue="{!v.inputError}">
            <div class="recordError">
                <ui:message title="Error" severity="error" closable="false">{!v.errorMessage}</ui:message>
            </div>
        </aura:if>
    </div>
    <div style="margin: auto;width: 75%;">
        <aura:if isTrue="{!v.showBillTo}">
            <fieldset class="slds-form--compound">
                <legend class="slds-text-heading--small slds-p-vertical--medium">Bill To Information </legend>
                <div class="form-element__group">
                    <aura:if isTrue="{!v.requiredError}">
                        <div class="recordError">
                            <ui:message title="Error" severity="error" closable="false"> Required Fields are missing.
                            </ui:message>
                        </div>
                    </aura:if>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;"> Account
                                Name&nbsp;</span> <strong><span style="color: #ff0000;font-weight:bold;">*</span>
                            </strong>
                            <lightning:input type="String" value="{!v.accRec.BillTo_Name__c}" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;"> Primary
                                Phone&nbsp;</span><a title="Enter 10 digits, no spacing or formatting required"><span
                                    style="color: #0000ff;font-weight:bold;"><strong>?</strong></span>&nbsp;<em><span
                                        style="color: #ff0000;font-weight:bold;">*</span></em></a> <span
                                onkeypress="{!c.validatePhoneNo}">
                                <lightning:input aura:id="Phone" value="{!v.accRec.Account_Phone__c}" type="tel" />
                            </span>
                            <p>
                                <lightning:formattedText aura:id="fteErrorP" class="error-text"
                                    value="Phone Required Field" />
                            </p>
                            <p>
                                <lightning:formattedText aura:id="fteErrorP1" class="error-text"
                                    value="Phone number should be 10 digits" />
                            </p>
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Street
                                Address <em><span style="color: #ff0000;font-weight:bold">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Street_Address_Bill__c}" maxlength="25" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Street
                                Address 2</span>
                            <lightning:input value="{!v.accRec.Street_Address_2_Bill__c}" maxlength="25" />
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">City<em><span
                                        style="color: #ff0000;font-weight:bold;"> *</span> </em> </span>
                            <lightning:input value="{!v.accRec.City_Bill__c}" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">State
                                <em><span style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.State_Bill__c}" maxlength="2" />
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Zip <em><span
                                        style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Zip_Bill__c}" maxlength="9" minlength="5" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Country
                                <em><span style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.BILL_COUNTRY__c}" maxlength="3" />
                        </div>
                    </div>
                </div>
            </fieldset>
            <lightning:button variant="brand" label="Next" onclick="{! c.handleClickNextBillTo}" disabled="{! v.validationError}"/>
        </aura:if>
    </div>
    <div style="margin: auto;width: 75%;">
        <aura:if isTrue="{!v.showShipTo}">
            <fieldset class="slds-form--compound">
                <legend class="slds-text-heading--small slds-p-vertical--medium">Ship To Information </legend>
                <div class="form-element__group">
                    <aura:if isTrue="{!v.requiredError}">
                        <div class="recordError">
                            <ui:message title="Error" severity="error" closable="false"> Required Fields are missing.
                            </ui:message>
                        </div>
                    </aura:if>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Street
                                Address <em><span style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Street_Address_Ship__c}" maxlength="25" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Street
                                Address 2</span>
                            <lightning:input value="{!v.accRec.Street_Address_2_Ship__c}" maxlength="25" />
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">City <em><span
                                        style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.City_Ship__c}" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">State
                                <em><span style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.State_Ship__c}" maxlength="2" />
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Zip <em><span
                                        style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Zip_Ship__c}" maxlength="9" minlength="5" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Country
                                <em><span style="color: #ff0000;font-weight:bold;">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.SHIP_COUNTRY__c}" maxlength="3" />
                        </div>
                    </div>
                </div>
            </fieldset>
            <lightning:button variant="brand" label="Back" onclick="{! c.handleClickBackShipTo}" />
            <lightning:button variant="brand" label="Next" onclick="{! c.handleClickNextShipTo}" disabled="{! v.validationError}"/>
        </aura:if>
    </div>
    <div style="margin: auto;width: 75%;">
        <aura:if isTrue="{!v.showPrimaryContact}">
            <fieldset class="slds-form--compound">
                <legend class="slds-text-heading--small slds-p-vertical--medium">Primary Contact Information </legend>
                <div class="form-element__group">
                    <aura:if isTrue="{!v.requiredError}">
                        <div class="recordError">
                            <ui:message title="Error" severity="error" closable="false"> Required Fields are missing.
                            </ui:message>
                        </div>
                    </aura:if>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">First Name
                                <em><span style="color: #ff0000;font-weight:bold">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Primary_Contact_First_Name__c}" maxlength="30" />
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Last Name
                                <em><span style="color: #ff0000;font-weight:bold">*</span> </em> </span>
                            <lightning:input value="{!v.accRec.Primary_Contact_Last_Name__c}" maxlength="30" />
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Phone</span><a
                                title="Enter 10 digits, no spacing for formatting required"><span
                                    style="color: #0000ff;font-weight:bold"><strong>?</strong></span>
                                <em><span style="color: #ff0000;font-weight:bold">*</span></em></a> <span
                                onkeypress="{!c.validatePhoneNo}">
                                <lightning:input value="{!v.accRec.Primary_Contact_Phone__c}" type="tel" /> </span>
                            <p>
                                <lightning:formattedText aura:id="fteErrorPCon" class="error-text"
                                    value="Phone Format (555) 555-5555" />
                            </p>
                            <p>
                                <lightning:formattedText aura:id="fteErrorP2" class="error-text"
                                    value="Phone number should be 10 digits" />
                            </p>
                        </div>
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Fax</span><a
                                title="Enter 10 digits, no spacing for formatting required"><span
                                    style="color: #0000ff;font-weight:bold"><strong>?</strong></span>
                            </a>
                            <lightning:input value="{!v.accRec.Primary_Contact_Fax__c}" />
                            <p>
                                <lightning:formattedText aura:id="fteErrorFax1" class="error-text"
                                    value="Fax Format (555) 555-5555" />
                            </p>
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-1"> <span
                                style="font-weight:bold;">Email</span><span style="font-weight:bold;"> <em><span
                                        style="color: #ff0000;font-weight:bold">*</span>
                                </em> </span>
                            <lightning:input aura:id="inputField" value="{!v.accRec.Primary_Contact_Email__c}"
                                messageWhenBadInput="Please Enter a valid Email address. (Email Address's cannot be an office Depot address)"
                                messageWhenPatternMismatch="Please Enter a valid Email address. (Email Address's cannot be an office Depot address)"
                                pattern="^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,10})+$" />
                            <p>
                                <lightning:formattedText aura:id="fteError" class="error-text"
                                    value="Please Enter a valid Email address. (Email Address's cannot be an office Depot address)" />
                            </p>
                        </div>
                    </div><span style="font-size: medium"><u><strong>ORDERING&nbsp;PREFERENCES </strong></u></span>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does your
                                Customer have any ordering preferences? <span
                                    style="color: #ff0000;font-weight:bold;">*</span></span>
                            <lightning:select aura:id="select" name="select"
                                value="{!v.accRec.Does_your_Customer_have_any_ordering_pr__c}" onchange="{!c.togglePO}">
                                <option value="">choose one... </option>
                                <option value="Yes">Yes </option>
                                <option value="No">No </option>
                            </lightning:select>
                        </div>
                    </div>
                    <aura:if isTrue="{!v.togglePO}">
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Purchase
                                    Order Field during Order Entry? </span>
                                <lightning:select aura:id="PurchaseOrder" name="PurchaseOrder" value="{!v.accRec.PO__c}"
                                    onchange="{!c.togglePOD}">
                                    <option value="">choose one...</option>
                                    <option value="Yes">Yes</option>
                                    <option value="Hide">Hide</option>
                                    <option value="Validate">Validate</option>
                                    <option value="No">No</option>
                                </lightning:select>
                            </div>
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Desktop
                                    Field during Order Entry? </span>
                                <lightning:select aura:id="DesktopField" name="DesktopField"
                                    onchange="{!c.togglePODeskTop}" value="{!v.accRec.Desktop__c}">
                                    <option value="">choose one...</option>
                                    <option value="Yes">Yes</option>
                                    <option value="Hide">Hide</option>
                                    <option value="Validate">Validate</option>
                                    <option value="No">No</option>
                                </lightning:select>
                            </div>
                        </div>
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Cost
                                    Center or Department Field during Order Entry? </span>
                                <lightning:select aura:id="CostCenter" name="CostCenter" onchange="{!c.togglePOCost}"
                                    value="{!v.accRec.Cost_Center__c}">
                                    <option value="">choose one...</option>
                                    <option value="Yes">Yes</option>
                                    <option value="Hide">Hide</option>
                                    <option value="Validate">Validate</option>
                                    <option value="No">No</option>
                                </lightning:select>
                            </div>
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;"> Release
                                    Field during Order Entry? </span>
                                <lightning:select aura:id="ReleaseField" name="ReleaseField"
                                    onchange="{!c.togglePORelease}" value="{!v.accRec.Release__c}">
                                    <option value="">choose one...</option>
                                    <option value="Yes">Yes</option>
                                    <option value="Hide">Hide</option>
                                    <option value="Validate">Validate</option>
                                    <option value="No">No</option>
                                </lightning:select>
                            </div>
                        </div><span style="font-weight:bold;"><u>Rename the following Field labels for Order Entry
                                &amp; Billing?</u></span>
                        <div class="slds-form-element__row">
                            <aura:if isTrue="{!v.togglePOD}">
                                <div class="slds-form-element slds-size--1-of-2"> <span
                                        style="font-weight:bold;">Purchase
                                        Order</span>
                                    <lightning:input aura:id="RenamePurchaseOrder" name="RenamePurchaseOrder"
                                        value="{!v.accRec.Rename_Purchase_Order__c}"
                                        placeholder="Limited to 7 Characters" maxlength="7" />
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.togglePODeskTop}">
                                <div class="slds-form-element slds-size--1-of-2"> <span
                                        style="font-weight:bold;">Desktop</span>
                                    <lightning:input aura:id="RenameDesktop" name="RenameDesktop"
                                        value="{!v.accRec.Rename_Desktop__c}" placeholder="Limited to 7 Characters"
                                        maxlength="7" />
                                </div>
                            </aura:if>
                        </div>
                        <div class="slds-form-element__row">
                            <aura:if isTrue="{!v.togglePOCost}">
                                <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Cost
                                        Center</span>
                                    <lightning:input aura:id="RenameCost" name="RenameCost"
                                        value="{!v.accRec.Rename_Cost_Center__c}" placeholder="Limited to 7 Characters"
                                        maxlength="7" />
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.togglePORelease}">
                                <div class="slds-form-element slds-size--1-of-2"> <span
                                        style="font-weight:bold;">Release</span>
                                    <lightning:input aura:id="RenameRelease" name="RenameRelease"
                                        value="{!v.accRec.Rename_Release__c}" placeholder="Limited to 7 Characters"
                                        maxlength="7" />
                                </div>
                            </aura:if>
                        </div>
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Minimum
                                    Order Value (MOV)</span> <br></br> <span class="ct_help ct_help_top">Prevents
                                    orders from being submitted unless the MOV is met</span>
                                <lightning:input aura:id="MOV" name="MOV" type="number"
                                    value="{!v.accRec.Minimum_Order_Value__c}" format="###,###.00" maxlength="8" />
                            </div>
                        </div>
                        <aura:if
                            isTrue="{!or(or(v.togglePORelease, v.togglePOCost),(or(v.togglePOD,v.togglePODeskTop)))}">
                            <div class="slds-form-element__row">
                                <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does
                                        the customer have budgets for each cost center or purchase order?</span>
                                    <lightning:select aura:id="Budget" name="Budget"
                                        value="{!v.accRec.Budgets_CC_PO__c}">
                                        <option value="">choose one... </option>
                                        <option value="Yes">Yes</option>
                                        <option value="No">No</option>
                                    </lightning:select>
                                </div>
                            </div>
                        </aura:if>
                    </aura:if>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does your
                                customer have any special Delivery Instructions?&nbsp;<strong><span
                                        style="color:#FF0000;font-weight:bold;">*&nbsp;</span>
                                </strong> </span>
                            <lightning:select aura:id="selectDelivery" name="selectDelivery"
                                value="{!v.accRec.Special_Delivery_Requirements__c}" onchange="{!c.toggleDelivery}">
                                <option value="">choose one... </option>
                                <option value="Yes">Yes </option>
                                <option value="No">No </option>
                            </lightning:select>
                        </div>
                    </div>
                    <aura:if isTrue="{!v.toggleDelivery}"> <span
                            style="font-size: large"><u>DELIVERY&nbsp;PREFERENCES</u></span>
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does a
                                    copy of the invoice need to be included with the packing list?</span>
                                <lightning:select aura:id="Invoice" name="Invoice" value="{!v.accRec.Print_Invoice__c}">
                                    <option value="">choose one... </option>
                                    <option value="Yes">Yes </option>
                                    <option value="No">No </option>
                                </lightning:select>
                            </div>
                        </div>
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Would you
                                    like to show the Purchase Order information on the packing list? </span>
                                <lightning:select aura:id="DisplayPO" name="DisplayPO"
                                    value="{!v.accRec.Display_Purchase_Orders__c}">
                                    <option value="">choose one... </option>
                                    <option value="Yes">Yes </option>
                                    <option value="No">No </option>
                                </lightning:select>
                            </div>
                        </div>
                        <div class="slds-form-element__row">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Would you
                                    like to show the Payment Method information on the packing list? </span>
                                <lightning:select aura:id="DisplayPM" name="DisplayPM"
                                    value="{!v.accRec.Display_Payment_Method__c}">
                                    <option value="">choose one... </option>
                                    <option value="Yes">Yes </option>
                                    <option value="No">No </option>
                                </lightning:select>
                            </div>
