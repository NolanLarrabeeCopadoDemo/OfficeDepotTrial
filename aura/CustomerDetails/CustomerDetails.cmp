    <div style="margin: auto;width: 100%;">
        <aura:if isTrue="{!v.showTax}">
            <fieldset class="slds-form--compound">
                <legend class="slds-text-heading--small slds-p-vertical--medium">Tax Section Information </legend>
                <div class="form-element__group">
                    <aura:if isTrue="{!v.requiredError}">
                        <div class="recordError">
                            <ui:message title="Error" severity="error" closable="false"> Required Fields are missing.
                            </ui:message>
                        </div>
                    </aura:if>
                </div>
                <div class="slds-form-element__row">
                    <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">TIN</span>
                        <lightning:input placeholder="Format XX-XXXXXXX" onblur="{!c.TINValid}"
                            pattern="[0-9]{2}-[0-9]{7}" value="{!v.accRec.Employer_TIN__c}" />
                    </div>
                </div>
                <div class="slds-form-element__row">
                <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Payment Method<br/>
                        <lightning:helptext
                                            content="Select the payment method your Customer needs to be setup on to remit payments"
                                            /></span>
                        <lightning:select aura:id="PaymentMethod" name="PaymentMethod" value="{!v.accRec.What_is_the_payment_method__c}">
                            <option value="">choose one... </option>
                            <option value="ACH">ACH</option>
                            <option value="CHECK">CHECK</option>
                        </lightning:select>
                    </div>
                </div>
                <div class="slds-form-element__row">
                    <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does this Customer offer CTX-820 or Email Remittance?<br/>
                        <lightning:helptext
                                            content="The customer will either email their remittance or it is incorporated in the ACH payment (CTX-820)"
                                            /></span>
                        <lightning:select aura:id="PaymentMethod" name="PaymentMethod" value="{!v.accRec.CTX_820_or_Email_Remittance__c}">
                            <option value="">choose one...</option>
                            <option value="CTX-820">CTX-820</option>
                            <option value="Email Remittance">Email Remittance</option>
                        </lightning:select>    
                    </div>
                </div>
                <div class="slds-form-element__row">
                    <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Does your
                            Customer require an IRS Federal W9 in order to pay us?</span>
                        <lightning:select aura:id="IRS" name="IRS" value="{!v.accRec.Is_IRS_W9_Required_for_AP__c}">
                            <option value="">choose one... </option>
                            <option value="Yes">Yes </option>
                            <option value="No">No </option>
                        </lightning:select>
                    </div>
                    <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Is your Customer
                            tax exempt?</span>
                        <lightning:select aura:id="EarlyPayDiscount3" name="EarlyPayDiscount"
                            value="{!v.accRec.Tax_Exempt__c}" onchange="{!c.toggletaxexempt}">
                            <option value="">choose one... </option>
                            <option value="Yes">Yes </option>
                            <option value="No">No </option>
                        </lightning:select> <span style="font-weight:bold;"><a target="_blank"
                                href="http://sp.na.odcorp.net/sites/BSG/PSBSD/Shared%20Documents/SFDC%20ACCOUNT%20SETUP%20DOCS/EXEMPTION%20MATRIX.xlsx">Click
                                here</a> to see a list of what items you will need.&nbsp; Also, to expedite the setup,
                            put in the comments section "Tax exempt customer" <br></br></span>
                    </div>
                </div>
                <aura:if isTrue="{!v.toggletax}">
                    <!-- Start: by Rajendra Shahane for NAIT-135726 02931356 - Allow Reps to Attach Tax Exemption Form - Account Setup -->
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-1"> <span style="font-weight:bold;">Upload your 
                            Customer tax exempt file/document</span>
                            <div style="width:50%">
                                <lightning:input aura:id="singlefileUpload" onchange="{!c.uploadtaxFile}" type="file" name="file" label="Upload File" multiple="false"/>
   								<div class="slds-text-body_small slds-text-color_error">Selected File: {!v.fileName} </div>
                            </div>
                        </div>
                    </div>
                    <!-- End: by Rajendra Shahane for NAIT-135726 02931356 - Allow Reps to Attach Tax Exemption Form - Account Setup -->
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-1"> <span style="font-weight:bold;">In which
                                state(s) are you exempt?</span>
                            <div style="width:50%">
                                <lightning:layout horizontalAlign="left" multipleRows="true">
                                    <aura:iteration items="{!v.checkboxValueMap}" var="cus" indexVar="key">
                                        <lightning:layoutItem size="2">{!cus.key}
                                            <lightning:input value="{!cus.key}" checked="{!cus.value}" label=""
                                                type="checkbox" onchange="{!c.onCheck}" aura:id="checkbox"
                                                class="myCheckbox" />
                                        </lightning:layoutItem>
                                    </aura:iteration>
                                </lightning:layout>
                            </div>
                        </div>
                    </div>
                    <div class="slds-form-element__row">
                        <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Type of tax
                                exemption:</span>
                            <lightning:select aura:id="exemption" name="exemption"
                                value="{!v.accRec.Why_are_you_tax_exempt__c}" onchange="{!c.togglegovt}">
                                <option value="0">Pick Exemption Type </option>
                                <option value="Government">Government </option>
                                <option value="Non-profit">Non-profit </option>
                                <option value="Reseller">Reseller </option>
                                <option value="School">School </option>
                            </lightning:select>
                        </div>
                        <aura:if isTrue="{!v.togglegov}">
                            <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Branch of
                                    government:</span>
                                <lightning:select aura:id="government" name="government"
                                    value="{!v.accRec.Goverment_Branch__c}">
                                    <option value=" ">Pick Government Branch </option>
                                    <option value="State">State </option>
                                    <option value="Local">Local </option>
                                    <option value="Federal">Federal </option>
                                </lightning:select>
                            </div>
                        </aura:if>
                    </div>
                    <aura:if isTrue="{!v.accRec.Show_Net_20__c}">
                        <div class="slds-form-element__row"> <span style="font-weight:bold;"><span
                                    style="color: #000080"><span style="font-size: larger"><strong>Copy and Paste the
                                            following in the comments
                                            section:<em>&nbsp;Please set my customer as tax
                                                exempt</em></strong></span></span>
                            </span> </div>
                    </aura:if>
                </aura:if>
            </fieldset>
            <lightning:button variant="brand" label="Back" onclick="{! c.handleClickBackTax}" />
            <lightning:button variant="brand" label="Next" onclick="{! c.handleClickNextTax}" disabled="{! v.validationError}"/>
        </aura:if>
    </div>
    <div style="margin: auto;width: 100%;">
        <aura:if isTrue="{!v.showSubmitRequest}">
            <div class="form-element__group">
                <aura:if isTrue="{!v.requiredError}">
                    <div class="recordError">
                        <ui:message title="Error" severity="error" closable="false"> Required Fields are missing.
                        </ui:message>
                    </div>
                </aura:if>
            </div>
            <fieldset class="slds-form--compound">
                <div class="slds-form-element__row" style="width: 50%;">
                    <div class="slds-form-element slds-size--1-of-2"> <span style="font-weight:bold;">Select DML</span>
                        <lightning:select aura:id="DML" required="false" value="{!v.accRec.DML__c}"
                            onchange="{!c.dmlselect}">
                            <option value="">Choose DML</option>
                            <aura:iteration items="{!v.DML}" var="cus" indexVar="key">
                                <option value="{!cus.value}" selected="{!cus.key==v.selectedDML}">{!cus.key}</option>
                            </aura:iteration>
                             <option value="New DML">New DML</option>
                        </lightning:select>
                    </div>
                </div>
                <aura:if isTrue="{!v.DMLs}">
                    <div class="slds-form-element__row" style="width: 50%;">
                        <div class="slds-form-element slds-size--1-of-1">
                            <lightning:button variant="brand" label="Suggest New DML"  disabled ="{!v.disableDMLCreate}" onclick="{! c.CreateDML}" />
                        </div>
                    </div>
                </aura:if>
                <aura:if isTrue="{!v.ALT}">
                    <div class="demo-only" style="height: 400px; width:100%;">
                        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                            aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                            <div class="slds-modal__container">
                                <!-- <lightning:input label="New DML Id" value="{!v.NewDMLId}"/> -->
                                <header class="slds-modal__header" style="width:100%;"> <button
                                        class="slds-button slds-modal__close slds-button--icon-inverse" title="Close"
                                        onclick="{!c.closeModel}"> X <span class="slds-assistive-text">Close</span>
                                    </button> </header>
                                <div style="background-color:white; width:100%; padding: 10px; font-weight: 900;">
                                    <aura:if isTrue="{!v.validationErrordml}">
                                        <div class="recordError">
                                            <ui:message title="Error" severity="error" closable="false"> {!v.DML_Error}
                                            </ui:message>
                                        </div>
                                    </aura:if>
                                    <div class="slds-form-element__row" style="width: 100%;">
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">Select
                                                Alternate DML</span>
                                            <lightning:select aura:id="AltDML" onchange="{!c.SelectAltDML}">
                                                <option value="">Choose DML</option>
                                                <aura:iteration items="{!v.AltDML}" var="cus" indexVar="key">
                                                    <option value="{!cus.value}">{!cus.key}</option>
                                                </aura:iteration>
                                            </lightning:select>
                                        </div>
                                    </div>
                                    <div class="slds-form-element__row">
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">DML
                                                Name</span>
                                            <lightning:input value="{!v.dmlRec.DECISION_MAKER_NM__c}" />
                                        </div>
                                    </div>
                                    <div class="slds-form-element__row">
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">DML
                                                City</span>
                                            <lightning:input value="{!v.dmlRec.DML_City__c}" />
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">DML
                                                State</span>
                                            <lightning:input value="{!v.dmlRec.DML_State__c}" />
                                        </div>
                                    </div>
                                    <div class="slds-form-element__row">
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">DML
                                                Zip</span>
                                            <lightning:input value="{!v.dmlRec.DECISION_MAKER_ZIP_CD__c}" />
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">Segment</span>
                                            <lightning:select aura:id="Segment"
                                                value="{!v.dmlRec.DECISION_MAKER_SEGMENT__c}">
                                                <option value="">Choose one Segment </option>
                                                <option value="Small &lt; $10K">Small &lt; $10K</option>
                                                <option value="Small $10K-$75K">Small $10K-$75K</option>
                                                <option value="Medium $75K-$150K">Medium $75K-$150K</option>
                                                <option value="Standard $150K-$250K">Standard $150K-$250K</option>
                                                <option value="Key $250K-$500K">Key $250K-$500K</option>
                                                <option value="Major $500K-$1M">Major $500K-$1M</option>
                                                <option value="K-12">K-12 </option>
                                                <option value="Higher-Ed">Higher-Ed </option>
                                                <option value="Federal">Federal </option>
                                                <option value="State">State </option>
                                                <option value="Local">Local </option>
                                                <option value="Non-Profit">Non-Profit </option>
                                                <option value="Enterprise $1M+">Enterprise $1M+</option>
                                            </lightning:select>
                                        </div>
                                    </div>
                                    <div class="slds-form-element__row">
                                        <div class="slds-form-element slds-size--1-of-2"> <span
                                                style="font-weight:bold;">Notes</span>
                                            <lightning:textarea aura:id="notesdml" value="{!v.dmlRec.Notes__c}" />
                                        </div>
                                    </div>
                                </div>
                                <footer class="slds-modal__footer" style="width:100%;">
                                    <lightning:button variant="brand" label="Submit Suggested DML"
                                        onclick="{! c.SubmitDML}" />
                                    <lightning:button variant="brand" label="Cancel" onclick="{! c.Cancel}" />
                                </footer>
                            </div>
                        </section>
                        <div class="slds-backdrop slds-backdrop_open"></div>
                    </div>
                </aura:if>
                <div style="width: 60%;">
                    <H1>*Attaching an account to the correct DML is important to ensure your account assignments are
                        properly aligned* </H1>
                    <p class="paragraph bold">
                        Select “Choose DML” to view if there is an existing DML for the account you are creating. If no
                        DML suggestion is available, or the suggested DML is not correct, please choose the “Suggest New
                        DML” option.
                    </p>
                    <p class="paragraph bold">
                        If you know there is an existing DML for the account that you are creating, please include that
                        information in the Comments section.
                    </p>
                    <p class="paragraph bold">
                        Note for Implementation Team: Please reference the name of the sales rep and the program that
                        you are creating the account for.
                    </p>
                </div>

                <div class="slds-form-element__row" style="width: 50%;">
                    <div class="slds-form-element slds-size--1-of-1">
                        <span style="font-weight:bold;"> Any Additional Comments about your Account Request?<br></br>
                            (<em>Note:
                                If you see comments in the box
                                below, do not delete them. You can append your comments to the end)</em></span>
                        <lightning:textarea name="message-input" value="{!v.accRec.Rep_Comments__c}" cols="40" rows="5"
                            maxlength="255" />
                    </div>
                </div>
                <div class="slds-form-element__row">
                    <div class="slds-form-element slds-size--1-of-1"> <span
                            style="font-size: x-large;font-weight:bold;">Please
                            click Submit to send this account for setup.</span> </div>
                </div>
            </fieldset>
            <lightning:button variant="brand" label="Back" onclick="{! c.handleClickBackSubmit}" />
            <lightning:button variant="brand" label="Submit" onclick="{! c.submitProspect}" disabled="{! v.validationError}"/>
            <!-- <lightning:button variant="brand" label="Close"onclick="{! c.SubmitProspectOnClose}"/> -->
        </aura:if>
    </div>
    <aura:if isTrue="{!v.isRecordCreated}">
        <div>Account Setup Request Submitted Successfully</div>
    </aura:if>
    <aura:if isTrue="{!v.validationError}">
        <div class="recordError">
            <ui:message title="Error" severity="error" closable="false">{!v.errorMessages}</ui:message>
        </div>
    </aura:if>
    <aura:if isTrue="{!v.Spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
            <div class="slds-spinner--brandslds-spinner slds-spinner--large slds-is-relative" role="alert"> <span
                    class="slds-assistive-text">Loading....
                </span>
                <div class="slds-spinner__dot-a"> </div>
                <div class="slds-spinner__dot-b"> </div>
            </div>
        </div>
    </aura:if>
</aura:component>