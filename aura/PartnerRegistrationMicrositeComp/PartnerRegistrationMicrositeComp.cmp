                                </div>
                            </div>
                            <aura:if isTrue="{!v.Three}">
                                <div class="slds-col slds-size_1-of-1 slds-large-size_1-of-1 slds-box slds-order_4 slds-medium-order_4 slds-large-order_4">
                                    <p id="showContent" style="background-color: #F1F3F4;text-align: center;"><img src="{!$Resource.Macro1 +'/images/Tile_3.png'}"></img></p>
                                </div>
