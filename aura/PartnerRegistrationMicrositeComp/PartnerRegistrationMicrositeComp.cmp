                        <div class="cross_bars" onclick="{!c.HamburgerMenu_close}">  <div class="bar1"></div>
                            <div class="bar2"></div><div class="bar3"></div></div><div style="clear:both;"></div>
                        <ul class="mainNavigation" id="mainNavigation" >
                            <li class="navLi" aura:id="OverviewLi"><a class="regularFont here" href="#overView" onmouseup="{!c.ScrollOverview}" aura:id="Overview" >Overview</a></li>
                            <li class="navLi" aura:id="BenefitsLi"><a class="regularFont here" href="#benefits" onmouseup="{!c.ScrollBenefits}" aura:id="Benefits">Benefits</a></li>
                            <li class="navLi" aura:id="SolutionsLi"><a class="regularFont here" href="#solutions" onmouseup="{!c.ScrollSolutions}" aura:id="Solutions" >Solutions</a></li>
                            <li class="navLi" aura:id="ProcessLi"><a class="regularFont here" href="#process" onmouseup="{!c.ScrollProcess}" aura:id="Process" >Process</a></li>
                            <li class="navLi" aura:id="EventsLi" ><a class="regularFont" href="#events" onmouseup="{!c.ScrollEvents}" aura:id="Events" >Events</a></li>
                            <li class="navLi" aura:id="PartnersLi"><a class="regularFont" href="#partners" onmouseup="{!c.ScrollPartners}"  aura:id="Partners">Partners</a></li>
                            <li class="navLi" style="display:none;" aura:id="ContactLi"><a class="regularFont" href="#contact" onmouseup="{!c.ScrollContact}"  aura:id="Contact">Contact</a></li>
