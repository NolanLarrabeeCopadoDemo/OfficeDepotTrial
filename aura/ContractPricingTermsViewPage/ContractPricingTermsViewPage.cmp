                                <span style="font-weight:bold;">Price increase unit type&nbsp;</span> <strong>
                                </strong>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3">
                                
                                <lightning:outputField variant = "label-hidden"  fieldName="Price_Incr_Cap_Type_Unit__c" />
                                
                            </lightning:layoutItem>
                        </lightning:layout>
                        <br></br>   
                            
                         
                            <lightning:layout horizontalAlign="left">
