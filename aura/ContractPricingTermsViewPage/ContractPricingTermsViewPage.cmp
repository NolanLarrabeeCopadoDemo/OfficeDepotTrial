                                <span style="font-weight:bold;">Index name(s)&nbsp;</span> <strong>
                                </strong>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3">
                                
                                <lightning:outputField variant = "label-hidden"  fieldName="Index_name_s__c" />
                                
                            </lightning:layoutItem>
                        </lightning:layout>
                        <br></br>  
                            </aura:if> 
                            
                            
                          
                            
                            <lightning:layout horizontalAlign="left">
                                <lightning:layoutItem size="5">
                                    <span style="font-weight:bold;">Price increase cap application level&nbsp;</span>
                                    <button class="slds-button slds-button_icon" title="Is the cap applied to each SKU individually, or as an average across multiple SKUs?">
                                        <lightning:icon class="slds-show" aura:id="infoicon" iconName="utility:info" size="x-small" alternativeText="Indicates dash"/>
                                    </button>
                                    <lightning:helptext content="Is the cap applied to each SKU individually, or as an average across multiple SKUs?" class="customIcon"/> 
                                    <strong>
                                    </strong>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="3">
                                    <lightning:outputField variant = "label-hidden"  fieldName="Price_Incr_Cap_App_Level__c" />
                                    
                                </lightning:layoutItem>
                            </lightning:layout>
                            <BR/>
                           
                            <BR/>
                            <lightning:layout horizontalAlign="left">
                                <lightning:layoutItem size="5">
                                    <span style="font-weight:bold;">Price increase cap period&nbsp;</span>
                                    <button class="slds-button slds-button_icon" title="What time horizon is the cap compliance evaluated on? (ex: increase should not exceed 5% annually would be annual; increase should not exceed 5% over 6 months = semi-annual)">
                                        <lightning:icon class="slds-show" aura:id="infoicon" iconName="utility:info" size="x-small" alternativeText="Indicates dash"/>
                                    </button>
                                    <lightning:helptext content="What time horizon is the cap compliance evaluated on? (ex: increase should not exceed 5% annually would be annual; increase should not exceed 5% over 6 months = semi-annual)" class="customIcon"/>
                                    <strong>
                                    </strong>
                                </lightning:layoutItem>
                                <BR/>
                                <lightning:layoutItem size="3">
                                    <lightning:outputField variant = "label-hidden"  fieldName="Price_Incr_Cap_Period__c" />
                                    
                                </lightning:layoutItem>
                            </lightning:layout>
                            <BR/>
                         
                        </aura:if> 
                        <br></br>
                    
                        
                    </div>
                    
                    
                </div>
                        </div>
               
