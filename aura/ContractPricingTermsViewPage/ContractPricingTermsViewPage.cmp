    <aura:attribute name="simpleCPT" type="Object"/>  
    <aura:attribute name="Displayviewmode" type="String" />
    <aura:registerEvent name="CPTEvent" type="c:ContractPricingTermsevt"/>
    <aura:attribute name="RecId" type="Id" access="global"/>
    <aura:attribute name="contractPricingTermsRecord" type="Contract_Pricing_Terms__c"/>
    <aura:attribute name="recordLoadError" type="String"/>
    <aura:attribute name="IsEdit" type="Boolean" default="false"/>
    <aura:attribute name="IsView" type="Boolean" default="true"/>
    <aura:attribute name="NoticePeriod" type="Boolean" default="false"/>
    <aura:attribute name="IsPriceCap" type="Boolean" default="false"/>
    <aura:attribute name="CPTrec" type="Contract_Pricing_Terms__c" default="{'sobjectType': 'Contract_Pricing_Terms__c'}" />
    <aura:attribute name="listOptions" type="List" default="[]"/>
    <aura:handler name="init" value="{!this}" action="{!c.myInit}"/>
    <aura:attribute name="recordError" type="String"/>
    <aura:attribute name="recordInfo" type="Object" />
    <aura:attribute name="simpleRecord" type="Object"/>
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="curView" type="String" />	
    <aura:attribute name="ClonePage" type="String" default="False" />	
