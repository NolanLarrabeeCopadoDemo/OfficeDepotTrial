                                <span style="font-weight:bold;">Complex cap&nbsp;</span> <strong>
                                </strong>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3">
                                
                                <lightning:outputField variant = "label-hidden"  fieldName="Complex_cap__c" />
                                
                            </lightning:layoutItem>
                        </lightning:layout>
                        <br></br>  
                            
                             <lightning:layout horizontalAlign="left">
                            <lightning:layoutItem size="5">
                                <span style="font-weight:bold;">Is price cap tied to an index?&nbsp;</span> <strong>
                                </strong>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3">
                                
                                <lightning:outputField variant = "label-hidden"  fieldName="Is_price_cap_tied_to_an_index__c" />
                                
                            </lightning:layoutItem>
                        </lightning:layout>
                        <br></br>  
                            
                            <aura:if isTrue="{!(v.fields.Is_price_cap_tied_to_an_index__c == 'Yes')}">
